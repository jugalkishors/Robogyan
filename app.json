{
  "expo": {
    "name": "Robogyan",
    "slug": "Robogyan",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "splash": {
      "image": "./assets/appsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.robogyan",
      "versionCode":4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
